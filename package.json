{
	"name": "local-operator-ui",
	"version": "0.4.6",
	"productName": "Local Operator",
	"description": "User interface for AI agent assistants that run Python code safely on your device through an intuitive chat interface. Supports local and cloud LLM models with built-in security features.",
	"main": "./out/main/index.js",
	"bin": {
		"local-operator-ui": "./bin/local-operator-ui.js"
	},
	"license": "GPL-3.0-or-later",
	"author": {
		"name": "Damian Tran",
		"email": "damian@local-operator.com",
		"url": "https://github.com/damianvtran"
	},
	"contributors": [
		{
			"name": "Damian Tran",
			"email": "damian@local-operator.com",
			"url": "https://github.com/damianvtran"
		}
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/damianvtran/local-operator-ui.git"
	},
	"homepage": "https://github.com/damianvtran/local-operator-ui#readme",
	"bugs": {
		"url": "https://github.com/damianvtran/local-operator-ui/issues"
	},
	"keywords": [
		"local-operator",
		"agent",
		"python",
		"tool use",
		"ui",
		"electron",
		"react"
	],
	"files": ["out", "bin", "LICENSE", "README.md"],
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"start": "electron-vite preview",
		"dev": "electron-vite dev",
		"build": "electron-vite build",
		"postbuild": "chmod +x ./bin/local-operator-ui.js",
		"lint": "npx @biomejs/biome check src",
		"lint:fix": "npx @biomejs/biome check --write src",
		"format": "npx @biomejs/biome format src",
		"format:fix": "npx @biomejs/biome format --write src",
		"prepublishOnly": "npm run build",
		"check-types": "tsc --noEmit",
		"pack": "pnpm run build && electron-builder --dir",
		"dist": "pnpm run build && electron-builder",
		"dist:mac": "pnpm run build && electron-builder --mac --publish never",
		"dist:win": "pnpm run build && electron-builder --win --publish never",
		"dist:linux": "pnpm run build && electron-builder --linux --publish never",
		"dist:all": "pnpm run build && electron-builder -mwl --publish never",
		"release": "pnpm run build && electron-builder --publish always",
		"generate-icons": "node scripts/generate-icons.js",
		"setup-python": "./scripts/setup-python-resource.sh",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "storybook build"
	},
	"dependencies": {
		"@electron-toolkit/utils": "^4.0.0",
		"@emotion/react": "^11.14.0",
		"@emotion/styled": "^11.14.0",
		"@fortawesome/fontawesome-svg-core": "^6.7.2",
		"@fortawesome/free-brands-svg-icons": "^6.7.2",
		"@fortawesome/free-solid-svg-icons": "^6.7.2",
		"@fortawesome/react-fontawesome": "^0.2.2",
		"@mui/material": "^6.4.2",
		"@tanstack/react-query": "^5.67.1",
		"@tanstack/react-query-devtools": "^5.67.1",
		"date-fns": "^4.1.0",
		"dmg-builder": "24.13.3",
		"dotenv": "^16.4.7",
		"electron-builder-squirrel-windows": "26.0.12",
		"electron-debug": "^3.2.0",
		"electron-log": "^4.4.8",
		"electron-updater": "^6.1.4",
		"html-react-parser": "^5.2.2",
		"katex": "^0.16.21",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-error-boundary": "^5.0.0",
		"react-markdown": "^10.1.0",
		"react-router-dom": "^6.16.0",
		"react-syntax-highlighter": "^15.6.1",
		"react-toastify": "^11.0.5",
		"rehype-katex": "^7.0.1",
		"remark-gfm": "^4.0.1",
		"remark-math": "^6.0.0",
		"styled-components": "^6.1.14",
		"uuid": "^11.1.0",
		"zod": "^3.24.2",
		"zustand": "^5.0.3"
	},
	"devDependencies": {
		"@babel/helpers": "^7.26.10",
		"@babel/runtime": "^7.26.10",
		"@biomejs/biome": "^1.9.4",
		"@chromatic-com/storybook": "^3.2.5",
		"@electron-toolkit/preload": "^3.0.1",
		"@electron-toolkit/tsconfig": "^1.0.1",
		"@electron/notarize": "^2.5.0",
		"@storybook/addon-essentials": "^8.6.4",
		"@storybook/addon-interactions": "^8.6.4",
		"@storybook/addon-onboarding": "^8.6.4",
		"@storybook/blocks": "^8.6.4",
		"@storybook/react": "^8.6.4",
		"@storybook/react-vite": "^8.6.4",
		"@storybook/test": "^8.6.4",
		"@types/react": "18.3.18",
		"@types/react-dom": "18.3.5",
		"@vitejs/plugin-react": "^4.3.4",
		"electron": "^25.3.1",
		"electron-builder": "^26.0.12",
		"electron-vite": "^3.0.0",
		"esbuild": "^0.25.1",
		"icns-lib": "^1.0.1",
		"jest": "^29.7.0",
		"prismjs": "^1.30.0",
		"sharp": "^0.33.2",
		"storybook": "^8.6.4",
		"typescript": "^5.8.2",
		"vite": "^6.2.0",
		"vite-tsconfig-paths": "^5.1.4",
		"yaml": "^2.7.0"
	},
	"peerDependencies": {
		"react": ">=18.0.0",
		"react-dom": ">=18.0.0"
	},
	"engines": {
		"node": ">=22.13.1"
	},
	"build": {
		"appId": "com.local-operator",
		"productName": "Local Operator",
		"copyright": "Copyright Â© 2025 Damian Tran",
		"asar": true,
		"files": ["out/**/*", "bin/**/*", "LICENSE", "README.md"],
		"directories": {
			"output": "dist",
			"buildResources": "build"
		},
		"extraResources": [
			{
				"from": "resources/python",
				"to": "python",
				"filter": ["**/*"]
			}
		],
		"afterSign": "scripts/notarize.js",
		"mac": {
			"notarize": false,
			"category": "public.app-category.developer-tools",
			"hardenedRuntime": true,
			"gatekeeperAssess": false,
			"entitlements": "build/entitlements.mac.plist",
			"entitlementsInherit": "build/entitlements.mac.plist",
			"target": [
				{
					"target": "dmg",
					"arch": ["universal"]
				},
				{
					"target": "zip",
					"arch": ["universal"]
				}
			],
			"artifactName": "${name}-${version}-${arch}.${ext}",
			"x64ArchFiles": "Contents/Resources/python/**/*"
		},
		"dmg": {
			"sign": false,
			"contents": [
				{
					"x": 130,
					"y": 220
				},
				{
					"x": 410,
					"y": 220,
					"type": "link",
					"path": "/Applications"
				}
			],
			"window": {
				"width": 540,
				"height": 380
			}
		},
		"win": {
			"target": [
				{
					"target": "nsis",
					"arch": ["x64", "arm64"]
				}
			],
			"artifactName": "${name}-setup-${version}-${arch}.${ext}",
			"verifyUpdateCodeSignature": false
		},
		"nsis": {
			"createDesktopShortcut": true,
			"createStartMenuShortcut": true,
			"deleteAppDataOnUninstall": true,
			"displayLanguageSelector": true,
			"multiLanguageInstaller": true
		},
		"linux": {
			"category": "ArtificialIntelligence",
			"icon": "build/",
			"target": [
				{
					"target": "deb",
					"arch": "x64"
				},
				{
					"target": "AppImage",
					"arch": "x64"
				},
				{
					"target": "rpm",
					"arch": "x64"
				}
			],
			"artifactName": "${name}-${version}-${arch}.${ext}",
			"desktop": {
				"entry": {
					"Name": "Local Operator",
					"Comment": "AI Agent Assistants On Your Device",
					"Categories": "ArtificialIntelligence"
				}
			}
		},
		"deb": {
			"depends": ["python3.12-full", "python3.12-venv"]
		},
		"rpm": {
			"depends": ["python3.12-full", "python3.12-venv"]
		},
		"publish": [
			{
				"provider": "github",
				"owner": "damianvtran",
				"repo": "local-operator-ui",
				"private": false,
				"releaseType": "release"
			}
		],
		"generateUpdatesFilesForAllChannels": true
	}
}
